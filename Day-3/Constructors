package com.Day3;

class Students {
    String name;
    int rno;
    String dept;
    static String college;

    // Parameterized Constructor (no need for college parameter)
    public Students(String name, int rno, String dept) {
        this.name = name;
        this.rno = rno;
        this.dept = dept;
    }

    // No-args Constructor
    public Students() {
        // Default constructor
    }

    public void display() {
        System.out.println("Name: " + name);
        System.out.println("Rno: " + rno);
        System.out.println("Dept: " + dept);
        System.out.println("College Name: " + college);
    }
}

public class ConstructorDemo {
    public static void main(String[] args) {
        // Set common college name
        Students.college = "Mohammed sathak A.J college of engineering";

        // Create first student using parameterized constructor
        Students s1 = new Students("Omar", 201, "Mechanical");
        s1.display();

        System.out.println("----------------------");

        // Create second student using no-args constructor
        Students s2 = new Students();
        s2.name = "Irfan";
        s2.rno = 202;
        s2.dept = "Computer Science";
        s2.display();
    }
}
